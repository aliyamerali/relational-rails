<h3>Bartenders</h3>

<% @bartenders.each do |bartender| %>
  <section id="bartender-<%= bartender.id %>">
    <ul>
      <li><%= bartender.name %> - started working on: <%= bartender.created_at %> <%= link_to "Edit", "/bartenders/#{bartender.id}/edit" %> <%= link_to "Delete Bartender", "/bartenders/#{bartender.id}", method: :delete %><br/><br/></li><br/>
    </ul>
  </section>
<% end %>

<%= link_to "New Bartender", "/bartenders/new" %>
