<h3>All Books at <%= @library.name %> Library: </h3>
<a href="/libraries/<%=@library.id%>/books">Sort Books Alphabetically</a>
<% @books.each do |book| %>
<h5><%= book.name %></h5>
<ul>
<li>Year Published: <%= book.publish_year %></li>
<li>Currently Available: <%= book.available %></li>
</ul>
<% end %>
<a href="/libraries/<%=@library.id%>/books/new">Create Book</a>
